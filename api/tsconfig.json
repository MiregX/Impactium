{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strictBindCallApply": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@prisma/*": [
        "./.prisma/*"
      ],
      "@api/main": [
        "src"
      ],
      "@api/main/*": [
        "src/*"
      ],
      "@seed": [
        "seed/src"
      ],
      "@seed/*": [
        "seed/src/*"
      ],
      "@seed/main/*": [
        "seed/src/api/*"
      ]
    }
  }
}